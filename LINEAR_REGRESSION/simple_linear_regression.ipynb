   "source": [
    "x = np.linspace(0, 1, 20)\n",
    "y = np.linspace(2,10, 20)\n",
    "x=x.reshape(-1,1)\n",
    "y=y.reshape(-1,1)\n",
    "x= np.c_[x,np.ones(x.shape)]\n",
    "a = np.dot(x.T,y)\n",
    "b = np.linalg.inv(np.dot(x.T,x))\n",
    "\n",
    "m,c = np.dot(b,a)\n",
    "\n",
    "print(m,c)"
   ]
